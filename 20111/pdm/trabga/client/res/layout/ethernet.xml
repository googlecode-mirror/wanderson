<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <TextView android:text="@string/ethernet_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/label_device_name"></TextView>
    <EditText android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/ethernet_name" android:text="@string/empty"></EditText>
    <TextView android:text="@string/ethernet_address" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/label_device_address"></TextView>
    <EditText android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/ethernet_address" android:text="@string/empty"></EditText>
    <TextView android:text="@string/ethernet_port" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/label_device_port"></TextView>
    <EditText android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/ethernet_port" android:text="@string/empty"></EditText>
</LinearLayout>
